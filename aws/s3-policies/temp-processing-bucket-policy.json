{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowLambdaAndMediaConvertAccess",
            "Effect": "Allow",
            "Principal": {
                "Service": [
                    "lambda.amazonaws.com",
                    "mediaconvert.amazonaws.com"
                ]
            },
            "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:DeleteObject",
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::sam-pautrat-temp-processing",
                "arn:aws:s3:::sam-pautrat-temp-processing/*"
            ]
        }
    ]
}
