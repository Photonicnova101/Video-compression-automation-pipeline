{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:DeleteObject",
                "s3:GetBucketLocation",
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::sam-pautrat-compressed-videos",
                "arn:aws:s3:::sam-pautrat-compressed-videos/*",
                "arn:aws:s3:::sam-pautrat-temp-processing",
                "arn:aws:s3:::sam-pautrat-temp-processing/*"
            ]
        }
    ]
}
