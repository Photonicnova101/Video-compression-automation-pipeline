{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowMediaConvertAccess",
            "Effect": "Allow",
            "Principal": {
                "Service": "mediaconvert.amazonaws.com"
            },
            "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:DeleteObject"
            ],
            "Resource": "arn:aws:s3:::sam-pautrat-compressed-videos/*"
        },
        {
            "Sid": "AllowLambdaAccess",
            "Effect": "Allow",
            "Principal": {
                "Service": "lambda.amazonaws.com"
            },
            "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:DeleteObject",
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::sam-pautrat-compressed-videos",
                "arn:aws:s3:::sam-pautrat-compressed-videos/*"
            ]
        }
    ]
}
